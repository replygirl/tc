{
  "name": "@replygirl/tc",
  "version": "0.0.0",
  "description": "destructurable, async-friendly `try...catch` wrapper function with support for error side effects and fallback values",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc ts-mocha",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/replygirl/tc.git"
  },
  "keywords": [
    "async",
    "await",
    "awaitable",
    "promise",
    "try",
    "catch",
    "wrapper",
    "function",
    "fallback",
    "error handling",
    "destructure",
    "destructuring",
    "functional",
    "utility",
    "helper"
  ],
  "author": {
    "name": "replygirl",
    "email": "moon@moon.fyi"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/replygirl/tc/issues"
  },
  "homepage": "https://github.com/replygirl/tc#readme",
  "devDependencies": {
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/mocha": "^8",
    "@types/node": "^14.0.27",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.x"
  },
  "files": [
    "dist/**/*"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  }
}
